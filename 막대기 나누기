a = int(input())

arr = [1,2,4,8,16,32]

cnt = 5
count = 0

if a == 64:
	print("1")

else:
	while(True):
		if (arr[cnt] > a):
			cnt = cnt - 1
            
		elif (arr[cnt] < a):
			a = a - arr[cnt]
			count = count + 1
			cnt = cnt - 1

		elif (a == arr[cnt]):
			count = count + 1
			break

	print(count)
